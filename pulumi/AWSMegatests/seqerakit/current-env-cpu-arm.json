{
  "discriminator": "aws-batch",
  "region": "eu-west-1",
  "executionRole": "arn:aws:iam::728131696474:role/TowerForge-5LWYX9a2GxrIFiax8tn9DV-ExecutionRole",
  "headJobRole": "arn:aws:iam::728131696474:role/TowerForge-5LWYX9a2GxrIFiax8tn9DV-FargateRole",
  "workDir": "s3://nf-core-awsmegatests",
  "headJobCpus": 4,
  "headJobMemoryMb": 16384,
  "environment": [],
  "waveEnabled": true,
  "fusion2Enabled": true,
  "nvnmeStorageEnabled": true,
  "fusionSnapshots": true,
  "nextflowConfig": "aws.batch.maxSpotAttempts=5\nprocess {\n    maxRetries = 2\n    errorStrategy = { task.exitStatus in ((130..145) + 104 + 175) ? 'retry' : 'terminate' }\n}\n",
  "forge": {
    "type": "SPOT",
    "minCpus": 0,
    "maxCpus": 1000,
    "gpuEnabled": false,
    "instanceTypes": ["m6gd", "r6gd", "c6gd"],
    "subnets": [],
    "securityGroups": [],
    "disposeOnDeletion": true,
    "allowBuckets": [
      "s3://ngi-igenomes",
      "s3://nf-core-awsmegatests",
      "s3://annotation-cache/"
    ],
    "efsCreate": false,
    "ebsBootSize": 50,
    "fargateHeadEnabled": false,
    "arm64Enabled": true
  },
  "labels": []
}
